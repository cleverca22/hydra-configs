{
  "enabled": 1,
  "hidden": false,
  "description": "A Nix based OS, but not NixOS",
  "nixexprinput": "configs",
  "nixexprpath": "not-os/default.nix",
  "checkinterval": 600,
  "schedulingshares": 100,
  "enableemail": false,
  "emailoverride": "",
  "keepnr": 10,
  "inputs":{
    "nixpkgs": { "type": "git", "value": "https://github.com/nixos/nixpkgs 21.05", "emailresponsible": false },
    "configs": { "type": "git", "value": "https://github.com/cleverca22/hydra-configs master", "emailresponsible": false },
    "pulls": { "type": "githubpulls", "value": "cleverca22 not-os", "emailresponsible": false }
  }
}
